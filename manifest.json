{
  "name": "Anti Rebind",
  "short_name": "AntiRebind",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Prevent DNS Rebinding Attacks",
  "homepage_url": "http://www.ThreatPinch.com",
  "icons": {
    "16": "icons/logo-tp16.png",
    "48": "icons/logo-tp48.png",
    "128": "icons/logo-tp128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "webRequest", "webRequestBlocking", "\u003Call_urls>"
  ],
  "web_accessible_resources": [ "blocked.html" ]
}
